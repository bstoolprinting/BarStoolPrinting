<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.barstoolprinting">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".AdminVendors"></activity>
        <activity android:name=".AdminJoin" />
        <activity android:name=".AdminAbout" />
        <activity android:name=".AdminProducts" />
        <activity android:name=".Vendors" />
        <activity android:name=".About" />
        <activity android:name=".AlexMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Directory"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Admin"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Products"
            android:windowSoftInputMode="stateHidden" />
    </application>

</manifest>